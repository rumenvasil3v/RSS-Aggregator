package rss;

import java.util.List;

public class FileReader {
	
	private List<String> urls;
	private List<Feed> feeds;
	
	public FileReader(List<String> urls) {
		
		
		for (String url: urls) {
			try {
				Feed feed = new Feed(url);
				
				this.feeds.add(feed);
			} catch (IllegalArgumentException e) {
				e.printStackTrace();
			}
		}
	}
}
